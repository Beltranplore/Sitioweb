    <style>
      .container {
        min-height: 100vh;
        
        @media screen and (min-width: 1024px) {
          height: 100vh;
          overflow-y: scroll;
        }
      }

      #content-container {
        @media screen and (min-width: 1024px) {
          margin: auto;
        }
      }

      #fields-container {
        transition: height 700ms;
        /* hide all fields until js loads in */
        display: none;
        overflow: visible;
      }

      .toggle {
        position: relative;
        display: inline-block;
        height: 44px;
        border-radius: 100px;
        padding: 0 4px;
        margin-bottom: 24px;
        color: transparent;
        font-size: 0;
        background-color: #e7e7e7;

        @media screen and (min-width: 1024px) {
          margin-bottom: 32px;
        }
      }

      .toggle input {
        appearance: none;
        -webkit-appearance: none;
        position: absolute;
      }

      .toggle input:focus-visible {
        ~ .slider {
          outline: 4px auto blue;
          outline: 4px auto -webkit-focus-ring-color;
        }
      }

      .toggle input:active {
        outline: none;
      }

      .slider {
        position: absolute;
        top: 0;
        left: var(--slider-left);
        height: 44px;
        border-radius: 100px;
        background-color: #fff;
        z-index: 2;
        transition: all 0.25s ease-in-out;
        border: 4px solid #e7e7e7;
        opacity: 0;
      }

      #toggle-label {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 44px;
        z-index: 1;
        cursor: pointer;
      }

      #toggle-label:hover + div {
        .inactive-text {
          color: #000;
        }
      }

      .content-container--login-default {
        #fields-container {
          height: var(--login-height);
          margin: 3px;
        }
      }

      .content-container--signup-default {
        #fields-container {
          box-sizing: border-box;
          padding: 3px;
          height: var(--signup-height);
        }
      }

      .fields-container--signup-active {
        height: var(--signup-height) !important;
      }

      .fields-container--login-active {
        height: var(--login-height) !important;
      }

      .text-container {
        z-index: 3;
        width: 100%;
        height: 44px;
        position: relative;
        top: 0;
        left: 0;
        display: flex;
        justify-content: space-between;
        align-items: center;
        flex-grow: 0;
        color: #000;
        text-transform: uppercase;
        font-family: Montserrat;
        font-size: 12px;
        font-weight: 700;
        line-height: 17px;
        pointer-events: none;
        padding: 0 32px;
        gap: 68px;
        text-align: center;
      }

      .inactive-text {
        color: #6e6e6e;
      }

      #login-container {
        animation: 1s cubic-bezier(0.42, 0, 0.28, 1);
      }

      #signup-container {
        animation: 1s cubic-bezier(0.42, 0, 0.28, 1);
      }
