            <span class="slider" role="presentation" id="slider" style="width: 124px; opacity: 1;"></span>
          </div>

          <div class="w-full" id="fields-container" style="display: block;">
            <div id="login-container" class="showing" style="display: block;">
              <form id="login-form">
                <div class="mb-5 w-full">
                  <label for="login-email" class="inline-block w-full mb-1 text-grey-a font-bold text-sm" data-translation="login.email.label">Email Address</label>

                  <input autocomplete="username" type="email" class="w-full border border-grey-g text-grey-a py-2 px-3 rounded" id="login-email" data-locator-id="loginRegister-loginEmailAddress-input" name="email" placeholder="Enter email address" data-translation="login.email.placeholder" required="">
                </div>

                <div class=" w-full">
                  <label for="current-password" class="inline-block w-full mb-1 text-grey-a font-bold text-sm" data-translation="login.password.label">Password</label>

                  <input autocomplete="current-password" type="password" class="w-full border border-grey-g text-grey-a py-2 px-3 rounded" id="current-password" data-locator-id="loginRegister-loginPassword-input" name="password" placeholder="Enter password" data-translation="login.password.placeholder" required="">
                  <div class="hidden text-error text-xs mt-1"></div>
                </div>

                <div class="w-full text-center mt-6">
                  <a href="https://www.gymshark.com/account/forgotten-password" id="login-forgotten-password" data-locator-id="loginRegister-forgottenPassword-select" class="font-bold underline text-sm" data-translation="login.button.forgotten-password.text">Forgot Password?</a>
                </div>

                <button type="submit" id="btn-login" data-locator-id="loginRegister-loginButton-select" class="w-full bg-black text-white font-heading font-bold text-sm uppercase p-3 rounded-full mt-6 hover:bg-black-b" data-translation="login.button.login.text">Log in</button>
              </form>
            </div>

            <div id="signup-container" style="display: none; opacity: 0;">
              <form id="signup-form" method="post" validate="">
                <div class="mb-5 w-full">
                  <label for="signup-firstname" class="inline-block w-full mb-1 text-grey-a font-bold text-sm" data-translation="signup.first-name.label">First name</label>

                  <input autocomplete="given-name" type="text" class="w-full border border-grey-g text-grey-a py-2 px-3 rounded" id="signup-firstname" data-locator-id="loginRegister-registerFirstName-input" name="firstname" placeholder="Enter first name" data-translation="signup.first-name.placeholder" required="">
                  <div class="firstname-validation hidden">
                    <span class="text-error text-xs mt-1" id="for-signup-firstname" data-translation="signup.firstname.requirements.title">
                      Please only use letters, spaces, dots and hyphens
                    </span>
                  </div>
                </div>

                <div class="mb-5 w-full">
                  <label for="signup-lastname" class="inline-block w-full mb-1 text-grey-a font-bold text-sm" data-translation="signup.last-name.label">Last name</label>

                  <input autocomplete="family-name" type="text" class="w-full border border-grey-g text-grey-a py-2 px-3 rounded" id="signup-lastname" data-locator-id="loginRegister-registerLastName-input" name="lastname" placeholder="Enter last name" data-translation="signup.last-name.placeholder" required="">
                  <div class="lastname-validation hidden">
                    <span class="text-error text-xs mt-1" id="for-signup-lastname" data-translation="signup.lastname.requirements.title">
                      Please only use letters, spaces, dots and hyphens
                    </span>
                  </div>
                </div>

                <div class="mb-5 w-full">
                  <label for="signup-email" class="inline-block w-full mb-1 text-grey-a font-bold text-sm" data-translation="signup.email.label">Email Address</label>

                  <input autocomplete="username" type="email" name="email" class="w-full border border-grey-g text-grey-a py-2 px-3 rounded" id="signup-email" data-locator-id="loginRegister-registerEmailAddress-input" placeholder="Enter email address" data-translation="signup.email.placeholder" required="">
                  <div class="hidden text-error text-xs mt-1"></div>
                </div>

                <div class="mb-5 w-full">
                  <label for="new-password" class="inline-block w-full mb-1 text-grey-a font-bold text-sm" data-translation="signup.password.label">Password</label>

                  <input autocomplete="new-password" type="password" name="password" class="w-full border border-grey-g text-grey-a py-2 px-3 rounded" id="new-password" data-locator-id="loginRegister-registerPassword-input" placeholder="Enter password" data-translation="signup.password.placeholder" required="">

                  <div class="password-validation hidden">
                    <span class="text-error text-xs mt-1" id="for-password" data-translation="signup.password.requirements.title">Your password does not match our requirements</span>

                    <ul class="flex flex-col md:flex-row md:flex-wrap text-grey-a text-sm pt-2">
                      <li class="flex md:w-1/2" id="pw_uppercase">
                        <div class="w-2 h-2 bg-grey-g rounded-full mr-2.5 mt-1 invalid"></div>
                        <span class="text-sm mr-1 material-symbols-outlined text-success valid hidden" data-translation="auth.signup.requirements.done">
                          done
                        </span>

                        <span data-translation="signup.password.requirements.uppercase">1 uppercase letter</span>
                      </li>

                      <li class="flex md:w-1/2" id="pw_characters">
                        <div class="w-2 h-2 bg-grey-g rounded-full mr-2.5 mt-1 invalid"></div>
                        <span class="text-sm mr-1 material-symbols-outlined text-success valid hidden" data-translation="auth.signup.requirements.done">
                          done
                        </span>

                        <span data-translation="signup.password.requirements.minimum-length">Minimum of 8 characters</span>
                      </li>

                      <li class="flex md:w-1/2" id="pw_lowercase">
                        <div class="w-2 h-2 bg-grey-g rounded-full mr-2.5 mt-1 invalid"></div>
                        <span class="text-sm mr-1 material-symbols-outlined text-success valid hidden" data-translation="auth.signup.requirements.done">
                          done
                        </span>

                        <span data-translation="signup.password.requirements.lowercase">1 lowercase letter</span>
                      </li>

                      <li class="flex md:w-1/2" id="pw_special">
                        <div class="w-2 h-2 bg-grey-g rounded-full mr-2.5 mt-1 invalid"></div>
                        <span class="text-sm mr-1 material-symbols-outlined text-success valid hidden" data-translation="auth.signup.requirements.done">
                          done
                        </span>

                        <span data-translation="signup.password.requirements.special">1 special character</span>
                      </li>

                      <li class="flex md:w-1/2" id="pw_number">
                        <div class="w-2 h-2 bg-grey-g rounded-full mr-2.5 mt-1 invalid"></div>
                        <span class="text-sm mr-1 material-symbols-outlined text-success valid hidden" data-translation="auth.signup.requirements.done">
                          done
                        </span>

                        <span data-translation="signup.password.requirements.number">1 number</span>
                      </li>
                    </ul>
                  </div>
                </div>

                <div class="flex gap-x-2 items-start">
                  <input type="checkbox" value="true" name="marketing[email][optin]" id="signup-email_optin" data-locator-id="loginRegister-registerEmailOptIn-select" class="mt-1 border border-black">

                  <label for="signup-email_optin" class="text-grey-a text-sm" data-translation="signup.marketing.consent">Tick here to receive emails about our products, apps, sales, exclusive content and more. View our <a href="https://support.gymshark.com/en-US/pages/privacy-notice-gs" class="underline font-bold text-black">Privacy Policy</a>.</label>
                </div>

                <button type="submit" id="btn-signup" data-locator-id="loginRegister-registerButton-select" class="w-full bg-black text-white font-heading font-bold text-sm uppercase p-3 rounded-full mt-4 hover:bg-black-b" data-translation="signup.button.create-account.text">Create account</button>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

    <script src="./cuenta_files/auth0.min.js.descarga"></script>
    <script src="./cuenta_files/object-assign.min.js.descarga"></script>
    <script nonce="V5DuPdz0/rhYyRoKSyt374B2w+RSQfy4RiUKjWsxJEo=">
      window.addEventListener("load", function() {
        const signupContainer = document.getElementById("signup-container");
        const loginContainer = document.getElementById("login-container");
        const loginText = document.getElementById("login-text");
        const signupText = document.getElementById("signup-text");
        const checkbox = document.getElementById("toggle-checkbox");
        const fieldsContainer = document.getElementById("fields-container");
        const slider = document.getElementById("slider");
        let setSignUpCSSVar = false
        let setLoginCSSVar = false

        const isDefaultTabSignup =
          new URL(window.location).searchParams.get("activeTab") === "signup"

        const setDefaultStyling = (
          activeElement,
          disabledElement,
          disabledText
        ) => {
          disabledElement.style.display = "none";
          disabledElement.style.opacity = 0;
          activeElement.style.display = "block";
          disabledText.classList.add("inactive-text");
          activeElement.classList.add("showing");
        };

        const updateHeightCssVar = (cssProperty, value) => {
          document.documentElement.style.setProperty(
            `--${cssProperty}-height`,
            value
          );
        }

        const updateSliderPositioning = (activeTab) => {
          if (activeTab === "login") {
            document.documentElement.style.setProperty("--slider-left", 0);
          } else {
            document.documentElement.style.setProperty(
              "--slider-left",
              document.getElementById("login-text").offsetWidth + 68 + "px"
            );
          }
        };

        // set styling for default open tab
        if (isDefaultTabSignup) {
          checkbox.checked = true;
          document
            .getElementById("content-container")
            .classList.add("content-container--signup-default");
          setDefaultStyling(signupContainer, loginContainer, loginText);

          // set estimate login height to enable first toggle animation
          updateHeightCssVar("login", "380px")
          // the signup height has been set after translations have been completed
          setSignUpCSSVar = true
        } else {
          document
            .getElementById("content-container")
            .classList.add("content-container--login-default");
          setDefaultStyling(loginContainer, signupContainer, signupText);
          updateSliderPositioning("login");

          // set estimate signup height so that it doesn't yank from 0 to true value on first toggle
          updateHeightCssVar("signup", "700px")
          // the login height has been set after translations have been completed
          setLoginCSSVar = true
        }
        fieldsContainer.style.display = "block";
        checkbox.ariaChecked = checkbox.checked;

        // change between showing signup and login fields on toggle change
        checkbox.addEventListener("change", () => {
          checkbox.ariaChecked = checkbox.checked;

          // animate field fade out by changing opacity and switch active text around
          if (checkbox.checked) {
            loginContainer.style.opacity = "0";
            updateSliderPositioning("signup");
            document
              .getElementById("signup-text")
              .classList.remove("inactive-text");
            document
              .getElementById("login-text")
              .classList.add("inactive-text");
          } else {
            signupContainer.style.opacity = "0";
            updateSliderPositioning("login");
            document
              .getElementById("login-text")
              .classList.remove("inactive-text");
            document
              .getElementById("signup-text")
              .classList.add("inactive-text");
          }

          if (checkbox.checked) {
            fieldsContainer.classList.remove("fields-container--login-active");
          } else {
            fieldsContainer.classList.remove("fields-container--signup-active");
          }

          // once inactive container has faded out remove inactive container and display active container
          setTimeout(() => {
            if (checkbox.checked) {
              fieldsContainer.classList.add("fields-container--signup-active");
              loginContainer.style.display = "none";
              signupContainer.style.display = "block";

              if (!setSignUpCSSVar) { 
                updateHeightCssVar("signup", `${document.getElementById("signup-container").offsetHeight}px`)
              }    
            } else {
                fieldsContainer.classList.add("fields-container--login-active");
                signupContainer.style.display = "none";
                loginContainer.style.display = "block";

              if (!setLoginCSSVar) { 
                updateHeightCssVar("login", `${document.getElementById("login-container").offsetHeight}px`)
              }
            }

            // set opacity on active container to 1 to trigger fade in animation
            setTimeout(() => {
              if (checkbox.checked) {
                signupContainer.style.opacity = "1";
              } else {
                loginContainer.style.opacity = "1";
              }
            }, 250);
          }, 250);
        });

        try {
          var config = JSON.parse(
            decodeURIComponent(escape(window.atob("")))
          );
          config.extraParams = config.extraParams || {};
          var realmValue = config.extraParams.realm;
          var params = Object.assign(
            {
              overrides: {
                __tenant: config.auth0Tenant,
                __token_issuer: "auth.gymshark.com",
              },
              domain: config.auth0Domain,
              clientID: config.clientID,
              redirectUri: config.callbackURL,
              responseType: "code",
            },
            config.internalOptions
          );

          var webAuth = new auth0.WebAuth(params);

          var databaseConnection = "Centralised-Login";

          function login(e) {
            e.preventDefault();
            var values = new FormData(e.target);

            const loginButton = document.getElementById("btn-login");
            loginButton.setAttribute("disabled", true);

            webAuth.login(
              {
                realm: realmValue,
                username: values.get("email"),
                password: values.get("password"),
              },
              function(err) {
                loginButton.removeAttribute("disabled");
                if (err) displayError(err);
              }
            );
          }

          function signup(e) {
            e.preventDefault();
            var values = new FormData(e.target);
            const locale = new URL(window.location).searchParams.get("locale");

            const signupButton = document.getElementById("btn-signup");
            signupButton.setAttribute("disabled", true);

            if (!handlePasswordCheck(values.get("password"))) {
              return signupButton.removeAttribute("disabled");
            }
            if (
              !handleNameCheck(
                values.get("firstname"),
                ".firstname-validation",
                "for-signup-firstname"
              )
            ) {
              return signupButton.removeAttribute("disabled");
            }
            if (
              !handleNameCheck(
                values.get("lastname"),
                ".lastname-validation",
                "for-signup-lastname"
              )
            ) {
              return signupButton.removeAttribute("disabled");
            }

            webAuth.redirect.signupAndLogin(
              {
                redirectUri: config.callbackURL,
                realm: realmValue,
                connection: databaseConnection,
                email: values.get("email"),
                password: values.get("password"),
                given_name: values.get("firstname"),
                family_name: values.get("lastname"),
                name: values.get("firstname") + " " + values.get("lastname"),
                userMetadata: {
                  locale: locale,
                  realm: realmValue,
                  acceptsMarketing: `${
                    document.getElementById("signup-email_optin").checked
                  }`,
                  acceptsMarketingUpdatedAt: Date.now().toString(),
                },
              },
              function(err, auth) {
                signupButton.removeAttribute("disabled");
                if (err) displayError(err);
              }
            );
          }

          function displayError(err) {
            var errorText = err.description;

            if (translationsData[`errors.${err.code}`]) {
              errorText = translationsData[`errors.${err.code}`];
            }

            if (err.code === "invalid_user_password") {
              var loginPasswordField = document.getElementById(
                "current-password"
              );
              var loginPasswordError = loginPasswordField.nextElementSibling;
              loginPasswordField.classList.add("border-b-2", "border-error");

              loginPasswordError.classList.remove("hidden");
              return (loginPasswordError.innerHTML = errorText);
            }

            if (err.code === "user_exists") {
              var signupEmailField = document.getElementById("signup-email");
              var signupEmailError = signupEmailField.nextElementSibling;
              signupEmailField.classList.add("border-b-2", "border-error");

              signupEmailError.classList.remove("hidden");
              return (signupEmailError.innerHTML = errorText);
            }

            var errorMessage = document.getElementById("error-message");
            errorMessage.innerHTML = errorText;
            errorMessage.classList.remove("hidden");
          }
          
          document
            .getElementById("logo-nav")
            .setAttribute("href", `${new URL(config.callbackURL).origin}`);
          document
          .getElementById("logo-nav-desktop")
          .setAttribute("href", `${new URL(config.callbackURL).origin}`);
  
          document
            .getElementById("login-forgotten-password")
            .setAttribute(
              "href",
              `${new URL(config.callbackURL).origin}/account/forgotten-password`
            );
        } catch (error) {
          console.log({ pageConfigError: error });
        }

        document.getElementById("login-form").addEventListener("submit", login);
        document
          .getElementById("signup-form")
          .addEventListener("submit", signup);

        document
          .getElementById("signup-email")
          .addEventListener("keyup", (e) => {
            e.target.classList.remove("border-b-2", "border-error");
            e.target.nextElementSibling.classList.add("hidden");
          });

        function nameCheck(name) {
          return /^[A-Za-z\s\u00C0-\u024F\u1E00-\u1EFF.\-'\p{Alphabetic}]+$/gu.test(
            name
          );
        }

        function minimumCharactersCheck(password) {
          return password.length >= 8;
        }

        function uppercaseCheck(password) {
          return /.*[A-Z]+.*/.test(password);
        }

        function lowercaseCheck(password) {
          return /.*[a-z]+.*/.test(password);
        }

        function specialCharacterCheck(password) {
          return /.*[!@#$%^&*(),.?":{}|<>]+.*/.test(password);
        }

        function numberCheck(password) {
          return /.*[0-9]+.*/.test(password);
        }

        function passwordValidation(password) {
          return (
            minimumCharactersCheck(password) &&
            uppercaseCheck(password) &&
            lowercaseCheck(password) &&
            specialCharacterCheck(password) &&
            numberCheck(password)
          );
        }

        var toggleSuccess = (parent, check) => {
          if (check) {
            parent.querySelector(".valid").classList.remove("hidden");
            return parent.querySelector(".invalid").classList.add("hidden");
          }

          parent.querySelector(".valid").classList.add("hidden");
          parent.querySelector(".invalid").classList.remove("hidden");
        };

        let handleNameCheck = (
          nameValue,
          nameValidationSelector,
          nameLabelFor
        ) => {
          let validationContainer = document.querySelector(
            nameValidationSelector
          );
          validationContainer.classList.remove("hidden");

          let errorSpan = document.getElementById(nameLabelFor);
          errorSpan.style.display = "inline-block";

          let isValidName = nameCheck(nameValue);

          errorSpan.style.color = isValidName ? "#9b9b9b" : "#BF2E35";

          if (isValidName) {
            validationContainer.classList.add("hidden");
            return true;
          }
          return false;
        };

        var handlePasswordCheck = (password) => {
          var container = document.querySelector(".password-validation");

          container.classList.remove("hidden");

          var errorSpan = document.getElementById("for-password");
          errorSpan.style.display = "inline-block";

          var characters = document.getElementById("pw_characters");
          var uppercase = document.getElementById("pw_uppercase");
          var lowercase = document.getElementById("pw_lowercase");
          var special = document.getElementById("pw_special");
          var num = document.getElementById("pw_number");

          toggleSuccess(characters, minimumCharactersCheck(password));
          toggleSuccess(uppercase, uppercaseCheck(password));
          toggleSuccess(lowercase, lowercaseCheck(password));
          toggleSuccess(special, specialCharacterCheck(password));
          toggleSuccess(num, numberCheck(password));

          errorSpan.style.color = passwordValidation(password)
            ? "#9b9b9b"
            : "#BF2E35";
          
            
          if (passwordValidation(password)) {
            container.classList.add("hidden");
            updateHeightCssVar("signup", `${document.getElementById("signup-container").offsetHeight}px`)
            return true;
          }
          updateHeightCssVar("signup", `${document.getElementById("signup-container").offsetHeight}px`)
          return false;
        };

        document
          .getElementById("new-password")
          .addEventListener("input", (e) =>
            handlePasswordCheck(e.target.value)
          );
        document
          .getElementById("signup-firstname")
          .addEventListener("input", (e) =>
            handleNameCheck(
              e.target.value,
              ".firstname-validation",
              "for-signup-firstname"
            )
          );
        document
          .getElementById("signup-lastname")
          .addEventListener("input", (e) =>
            handleNameCheck(
              e.target.value,
              ".lastname-validation",
              "for-signup-lastname"
            )
          );
      });
    </script>
  

</body></html>