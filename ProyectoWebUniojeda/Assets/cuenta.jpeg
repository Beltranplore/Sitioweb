    <script nonce="cUIIjYUTWMRr2TFQXHhB3qhmKyi5KkAJlWUNfW8c1Vk=">
      const translations = () => {
        var locale = new URL(window.location).searchParams.get("locale");

        fetch(
          `https://config.gymshark.io/translations/${
            locale ? locale : "en-GB"
          }/auth.json`
        )
          .then((data) => data.json())
          .then((translations) => {
            window.translationsData = translations;
            Object.entries(translations).map(
              ([translationKey, translationValue]) => {
                var elements = document.querySelectorAll(
                  `[data-translation="${translationKey}"]`
                );

                if (elements && translationValue.length > 0) {
                  if (translationKey === 'signup.marketing.consent') {
                    translationValue = translationValue.replace(
                      "<1>",
                      '<a href="https://support.gymshark.com/en-US/pages/privacy-notice-gs" class="underline font-bold text-black">'
                    );
                    translationValue = translationValue.replace("</1>", "</a>");
                  }

                  elements.forEach((element) => {
                    if (
                      translationKey.includes("placeholder") &&
                      element.localName === "input"
                    ) {
                      element.setAttribute("placeholder", translationValue);
                    } else {
                      element.innerHTML = translationValue;
                    }
                  });
                }
              }
            );
          }).finally(() => {
            const loginLength = document.getElementById("login-text")
              .offsetWidth;
            const registerLength = document.getElementById("signup-text")
              .offsetWidth;

            // ensure the toggles are equal width
            if (loginLength > registerLength)
              document.getElementById(
                "signup-text"
              ).style.width = `${loginLength}px`;
            if (registerLength > loginLength)
              document.getElementById(
                "login-text"
              ).style.width = `${registerLength}px`;

            slider.style.width =
              document.getElementById("login-text").offsetWidth + 72 + "px";

            if (
                new URL(window.location).searchParams.get("activeTab") ===
                "signup"
              ) {
                document.documentElement.style.setProperty(
                  "--signup-height",
                  `${document.getElementById("signup-container").offsetHeight}px`
                );

                document.documentElement.style.setProperty(
                  "--slider-left",
                  document.getElementById("login-text").offsetWidth + 68 + "px"
                  );
              } else {
                document.documentElement.style.setProperty(
                  "--login-height",
                  `${document.getElementById("login-container").offsetHeight}px`
                );
            }

            document.getElementById("slider").style.opacity = 1;
          });
      };

      tailwind.config = {
        theme: {
          fontFamily: {
            heading: ["Montserrat"],
            body: ["Roboto"],
          },
          borderWidth: {
            DEFAULT: "1px",
            "3": "3px",
          },
          extend: {
            fontSize: {
              "5xl": "2.8125rem",
            },
            colors: {
              "black-b": "#1B1B1B",
              "grey-a": "#444444",
              "grey-c": "#6E6E6E",
              "grey-f": "#EBEBEB",
              "grey-g": "#BBBCBC",
              error: "#BF2E35",
              success: "#2C8349",
            },
          },
        },
      };

      window.addEventListener("load", function() {
        translations();

        var currentSlide = 0;
        var slides = document.querySelectorAll(".slide");
        var slideIndicators = document.querySelectorAll(".slide-indicator");

        var updateSlides = (newIndex) => {
          if (newIndex !== undefined) {
            currentSlide = newIndex;
          }

          for (var i = 0; i < slides.length; i++) {
            slides[i].classList.add("hidden");
            slideIndicators[i].classList.add("opacity-50");

            if (currentSlide === i) {
              slides[i].classList.remove("hidden");
              slideIndicators[i].classList.remove("opacity-50");
            }
          }

          if (currentSlide === slides.length - 1) {
            return (currentSlide = 0);
          }

          return currentSlide++;
        };

        // Call function first before setting interval
        updateSlides();

        var interval = 6000;
        var sliderTimer = setInterval(updateSlides, interval);

        for (var i = 0; i < slideIndicators.length; i++) {
          slideIndicators[i].addEventListener("click", (e) => {
            clearInterval(sliderTimer);
            sliderTimer = setInterval(updateSlides, interval);

            updateSlides(
              Array.from(e.target.parentNode.children).indexOf(e.target)
            );
          });
        }
      });
    </script>
